{
  "name": "group-boticario-cashback",
  "version": "1.0.0",
  "description": "Test group boticario",
  "main": "index.js",
  "repository": "https://github.com/brunobarbosagoncalves/group-boticario-cashback.git",
  "author": "Bruno Barbosa Goncalves <brunoti.ads@gmail.com>",
  "license": "MIT",
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "yarn run husky-precommit",
      "pre-push_x": "yarn run husky-prepush"
    }
  },
  "scripts": {
    "db-run-migration": "yarn cross-env NODE_ENV=development sequelize --options-path=.sequelizerc.js db:migrate",
    "db-run-migration-undo-dev": "yarn cross-env NODE_ENV=development sequelize --options-path=.sequelizerc.js db:migrate:undo",
    "db-run-seeders": "yarn cross-env NODE_ENV=development sequelize --options-path=.sequelizerc.js db:seed:all",
    "db-run-seeders-undo": "yarn cross-env NODE_ENV=development sequelize --options-path=.sequelizerc.js db:seed:undo:all",
    "db-run-migration-undo-all": "yarn cross-env NODE_ENV=development sequelize --options-path=.sequelizerc.js db:migrate:undo:all",
    "husky-precommit": "yarn run eslint",
    "husky-prepush": "",
    "install-libs-prod": "yarn install-libs-global-prod && yarn install-libs-local",
    "install-libs-dev": "yarn install-libs-global-dev && yarn install-libs-local",
    "install-libs-global-prod": "yarn global add pm2 cross-env",
    "install-libs-global-dev": "yarn global add nodemon cross-env",
    "install-libs-local": "yarn install",
    "eslint": "./node_modules/.bin/eslint src --fix --max-warnings=0 ",
    "build": "yarn cross-env NODE_ENV=production && yarn babel src -d build --out-file-extension .js --source-maps inline",
    "test": "yarn cross-env NODE_ENV=development",
    "test-coverage": "yarn run test && jest --coverage",
    "jest-watch": "yarn run test && jest --watchAll",
    "kill-server": "pm2 stop my-api",
    "prod:monit": "yarn run prod && pm2 monit",
    "prod": "yarn install-libs-prod && yarn run build && yarn cross-env NODE_ENV=production BABEL_ENV=production pm2 start ./build",
    "prod:docker": "yarn install-libs-prod && yarn run build && yarn cross-env NODE_ENV=production BABEL_ENV=production pm2-runtime start ./build",
    "dev": "yarn install-libs-dev && yarn cross-env NODE_ENV=development BABEL_ENV=debug nodemon --exec babel-node src"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/runtime": "^7.9.6",
    "axios": "^0.21.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "husky": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.25.3",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.2",
    "pm2": "^4.4.0",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.5",
    "response-time": "^2.3.2",
    "sequelize": "^5.21.3",
    "swagger-ui-express": "^4.1.4",
    "uuid": "7.0.3",
    "uuidv4": "^6.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^6.0.0"
  }
}
